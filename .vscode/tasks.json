{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "flutter",
      "command": "flutter",
      "args": ["pub", "get"],
      "problemMatcher": [],
      "label": "Sideswap Endpoint: flutter pub get",
      "dependsOrder": "sequence",
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
        "type": "flutter",
        "command": "dart",
