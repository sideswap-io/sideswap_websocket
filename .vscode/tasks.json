        "problemMatcher": [
            "$dart-build_runner"
        ],
        "group": {
            "kind": "build",
            "isDefault": true
        },
        "label": "Sideswap Endpoint: dart pub run build_runner build",
        "dependsOrder": "sequence",
        "dependsOn": [
            "Sideswap Endpoint: flutter pub get"
        ],
        "options": {
            "cwd": "${workspaceFolder}"
        }
    },
  ]
}
